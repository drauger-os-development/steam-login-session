{
    "steam_type": {
        "flatpak": [
                "/usr/bin/flatpak", "run", "--branch=stable", "--arch=x86_64", "--command=/app/bin/steam",
                "--file-forwarding", "com.valvesoftware.Steam"
            ],
        "snap": ["/snap/bin/steam"],
        "native": ["/usr/bin/steam"]
    },
    "window_manager": {
        "xfwm4":{
                "command": ["/usr/bin/xfwm4", "--replace", "--compositor="],
                "supports_comp": true
            },
        "openbox": {
                "command": ["/usr/bin/openbox", "--replace"],
                "supports_comp": false
            },
        "kwin_x11": {
                "command": ["/usr/bin/kwin_x11", "--replace"],
                "supports_comp": false
            }
    }
}
